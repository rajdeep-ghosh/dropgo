{
  "name": "dropgo",
  "version": "0.1.0",
  "private": true,
  "author": "Rajdeep Ghosh",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "db:generate": "drizzle-kit generate",
