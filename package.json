    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@upstash/ratelimit": "^2.0.1",
    "@upstash/redis": "^1.33.0",
    "drizzle-orm": "^0.31.4",
    "lucide-react": "^0.378.0",
    "mime-types": "^2.1.35",
    "next": "^14.1.2",
    "react": "^18",
    "react-dom": "^18",
    "zod": "^3.23.8",
    "zod-error": "^1.5.0"
